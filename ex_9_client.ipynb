    "import socket\n",
    "c=socket.socket()\n",
    "host=socket.gethostname()\n",
    "port=6666\n",
    "c.connect((host,port))\n",
    "while True:\n",
    "   cmd=input(\"enter the command :\")\n",
    "   c.send(cmd.encode())\n",
    "   print(\"command has been sent to the server\")\n",
    "   break"
   ]
